{
	"name": "directus-extension-LMS-project-bundle",
	"description": "Please enter a description for your extension",
	"icon": "extension",
	"version": "1.0.0",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-extension-bundle"
	],
	"type": "module",
	"files": [
		"dist"
	],
	"directus:extension": {
		"type": "bundle",
		"path": {
			"app": "dist/app.js",
			"api": "dist/api.js"
		},
		"entries": [
			{
				"type": "hook",
				"name": "automatic-grading-hook",
				"source": "src/automatic-grading-hook/index.ts"
			},
			{
				"type": "hook",
				"name": "course-bookmark-hook",
				"source": "src/course-bookmark-hook/index.ts"
			},
			{
				"type": "hook",
				"name": "exercise-distribute-papers-hook",
				"source": "src/exercise-distribute-papers-hook/index.ts"
			},
			{
				"type": "hook",
				"name": "paper-section-question-group-hook",
				"source": "src/paper-section-question-group-hook/index.ts"
			},
			{
				"type": "endpoint",
				"name": "question-results-submit-queue-endpoint",
				"source": "src/question-results-submit-queue-endpoint/index.ts"
			},
			{
				"type": "hook",
				"name": "student-to-user-hook",
				"source": "src/student-to-user-hook/index.ts"
			},
			{
				"type": "endpoint",
				"name": "fetch-paper-cache-endpoint",
				"source": "src/fetch-paper-cache-endpoint/index.ts"
			},
			{
				"type": "hook",
				"name": "set-paper-cache-hook",
				"source": "src/set-paper-cache-hook/index.ts"
			},
			{
				"type": "hook",
				"name": "set-practice-session-cache-hook",
				"source": "src/set-practice-session-cache-hook/index.ts"
			}
		],
		"host": "^10.10.0"
	},
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link",
		"validate": "directus-extension validate",
		"add": "directus-extension add"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "13.1.0",
		"@types/node": "^22.15.21",
		"typescript": "^5.8.3"
	},
	"dependencies": {
		"@directus/extensions": "^3.0.5",
		"@directus/types": "^13.1.1",
		"bullmq": "^5.53.0",
		"ioredis": "^5.6.1"
	},
	"engines": {
		"node": ">=20.0.0",
		"pnpm": ">=9"
	}
}
